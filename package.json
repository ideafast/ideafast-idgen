{
    "name": "@ideafast/idgen",
    "version": "0.1.0",
    "description": "ID generators for the IDEAFast study",
    "main": "./dist/lib.umd.js",
    "module": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "repository": "https://github.com/ideafast/ideafast-idgen.git",
    "author": "Florian Guitton <f.guitton@imperial.ac.uk>",
    "license": "MIT",
    "keywords": [
        "ideafast",
        "id",
        "generator"
    ],
    "scripts": {
        "test": "jest --coverage",
        "test:mutation": "stryker run ./config/stryker.config.js",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx ./config ./src",
        "build": "tsc -p . && webpack --config ./config/webpack.config.js",
        "start": "webpack --config ./config/webpack.config.js --progress --color --watch",
        "prepare": "npm run build"
    },
    "devDependencies": {
        "@babel/core": "7.10.3",
        "@babel/preset-env": "7.10.3",
        "@babel/preset-typescript": "7.10.1",
        "@stryker-mutator/core": "3.3.0",
        "@stryker-mutator/javascript-mutator": "3.3.0",
        "@stryker-mutator/jest-runner": "3.3.0",
        "@stryker-mutator/typescript": "3.3.0",
        "@types/jest": "26.0.3",
        "@typescript-eslint/eslint-plugin": "3.4.0",
        "@typescript-eslint/parser": "3.5.0",
        "babel-loader": "8.1.0",
        "babel-plugin-add-module-exports": "1.0.2",
        "babel-plugin-rewire-ts": "1.3.3",
        "eslint": "7.3.1",
        "eslint-loader": "4.0.2",
        "eslint-plugin-jest": "23.17.1",
        "jest": "26.1.0",
        "rimraf": "3.0.2",
        "typescript": "3.9.5",
        "uglifyjs-webpack-plugin": "2.2.0",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.12"
    },
    "babel": {
        "extends": "./config/babel.config.js"
    },
    "eslintConfig": {
        "extends": "./config/eslint.config.js"
    }
}